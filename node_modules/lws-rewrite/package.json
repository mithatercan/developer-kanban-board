{
  "name": "lws-rewrite",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "3.1.1",
  "description": "lws middleware plugin adding URL rewriting support",
  "repository": "https://github.com/lwsjs/rewrite.git",
  "license": "MIT",
  "keywords": [
    "lws",
    "lws-middleware",
    "url",
